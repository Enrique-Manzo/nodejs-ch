<div class="sub_page">
  {{> header}}
      <div class="limiter">
		<div class="container-login100" style="background-image: url('images/bg-01.jpg');">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
				<form class="login100-form validate-form" id="registration-form">
					<h1 class="login100-form-title p-b-49">
						Register
					</h1>
					<div id="random-avatar">

					</div>
					<div class="wrap-input100 validate-input m-b-23">
						<span class="label-input100">Upload a profile picture</span>
						<input class="input100" type="file" name="profile_picture" id="profile_picture">
					</div>
					<div class="wrap-input100 validate-input m-b-23" data-validate = "Username is required">
						<span class="label-input100">Email address</span>
						<input class="input100" type="text" name="username" placeholder="Enter your email" id="login_username">
					</div>

					<div class="wrap-input100 validate-input" data-validate="Password is required">
						<span class="label-input100">Password</span>
						<input class="input100" type="password" name="pass" placeholder="Choose your password" id="login_password">
					</div>
                    <div class="wrap-input100 validate-input" data-validate="Password is required">
						<span class="label-input100">Repeat Password</span>
						<input class="input100" type="text" name="pass" placeholder="Repeat your password">
					</div>
					<div>
						<div class="wrap-input100 validate-input" data-validate="Your first name is required">
							<span class="label-input100">First Name</span>
							<input class="input100" type="text" name="fname" placeholder="Jane" id="fname">
						</div>
						<div class="wrap-input100 validate-input" data-validate="Your last name is required">
							<span class="label-input100">Last Name</span>
							<input class="input100" type="text" name="lname" placeholder="Smith" id="lname">
						</div>
					</div>
					<div class="wrap-input100 validate-input" data-validate="Your address is required">
							<span class="label-input100">Address</span>
							<input class="input100" type="text" name="address" placeholder="Evegreen ave. No 320" id="address">
					</div>
					<div class="wrap-input100 validate-input" data-validate="Your phone number is required">
							<span class="label-input100">Phone number</span>
							<input class="input100" type="text" name="lname" placeholder="+5422433092" id="phoneNo">
					</div>
					<div class="wrap-input100 validate-input" data-validate="Your age is required">
							<span class="label-input100">Age</span>
							<input class="input100" type="text" name="lname" placeholder="31" id="age">
					</div>					
					<div class="container-login100-form-btn m-t-50" id="button_container">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
							<button class="login100-form-btn" id="registration_button" type="button">
								CREATE ACCOUNT
							</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
	<div id="dropDownSelect1"></div>
{{> footer}}
{{> scripts}}
</div>